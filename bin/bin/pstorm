declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/martes/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm/ch-0/223.7571.212/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"